{
  "name": "example",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./config/migration.config.ts",
      "./dist/apps/database/config/migration.config.js"
    ]
  },
  "scripts": {
    "clear": "rimraf dist",
    "build": "nest build",
    "build:admin": "NODE_OPTIONS=--max_old_space_size=3072 nest build admin",
    "build:client": "NODE_OPTIONS=--max_old_space_size=3072 nest build client",
    "build:database": "NODE_OPTIONS=--max_old_space_size=3072 nest build database",
    "postbuild:client": "cp --parents `find apps  \\( -name \"*.md\" \\)` dist/apps/client/",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "NODE_OPTIONS=--max_old_space_size=4096 ./node_modules/eslint/bin/eslint.js \"{src,apps,libs,test,database}/**/*.ts\" --fix",
    "lint-errors": "NODE_OPTIONS=--max_old_space_size=4096 ./node_modules/eslint/bin/eslint.js \"{src,apps,libs,test,database}/**/*.ts\" --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest.js --runInBand",
    "test:e2e:admin": "node --max_old_space_size=5120 --expose-gc ./node_modules/jest/bin/jest.js --config ./apps/admin/test/jest-e2e.json --runInBand --detectOpenHandles --logHeapUsage --verbose",
    "test:e2e:admin:dist": "node --max_old_space_size=5120 --expose-gc ./node_modules/jest/bin/jest.js --config ./dist/apps/admin/apps/admin/test/jest-e2e.dist.json --runInBand --detectOpenHandles --logHeapUsage --verbose",
    "test:e2e:client": "node --max_old_space_size=3072 --expose-gc ./node_modules/jest/bin/jest.js --config ./apps/client/test/jest-e2e.json --runInBand --detectOpenHandles --logHeapUsage --verbose",
    "test:e2e:client:dist": "node --max_old_space_size=3072 --expose-gc ./node_modules/jest/bin/jest.js --config ./dist/apps/client/apps/client/test/jest-e2e.dist.json --runInBand --detectOpenHandles --logHeapUsage --verbose",
    "test:e2e:queue-processor": "node --max_old_space_size=3072 --expose-gc ./node_modules/jest/bin/jest.js --config ./apps/queue-processor/test/jest-e2e.json --runInBand --detectOpenHandles --logHeapUsage --forceExit",
    "test:e2e:queue-processor:dist": "node --max_old_space_size=3072 --expose-gc ./node_modules/jest/bin/jest.js --config ./dist/apps/queue-processor/apps/queue-processor/test/jest-e2e.dist.json --runInBand --detectOpenHandles --logHeapUsage --forceExit",
    "seed:run": "node ./dist/apps/database/apps/database/src/console.js seed run",
    "migration:up": "node -r ./dist/apps/database/apps/dotenv.js -r tsconfig-paths/register -r ts-node/register node_modules/@mikro-orm/cli/cli.js migration:up",
    "migration:up:all": "CONNECTION=Default npm run migration:up",
    "migration:down": "node -r ./dist/apps/database/apps/dotenv.js -r tsconfig-paths/register -r ts-node/register node_modules/@mikro-orm/cli/cli.js migration:down",
    "migration:create": "node -r ./dist/apps/database/apps/dotenv.js -r tsconfig-paths/register -r ts-node/register node_modules/@mikro-orm/cli/cli.js migration:create",
    "cli-database": "node dist/apps/database/apps/database/src/console.js"
  },
