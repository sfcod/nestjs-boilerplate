  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@libs/core(|/.*)$": "<rootDir>/libs/core/src",
      "^@libs/access-key(|/.*)$": "<rootDir>/libs/access-key/src",
      "^@libs/file-storage(|/.*)$": "<rootDir>/libs/file-storage/src",
      "^@libs/file-uploader(|/.*)$": "<rootDir>/libs/file-uploader/src",
      "^@libs/mailer(|/.*)$": "<rootDir>/libs/mailer/src",
      "^@libs/notification(|/.*)$": "<rootDir>/libs/notification/src",
      "^@libs/pusher(|/.*)$": "<rootDir>/libs/pusher/src",
      "^@libs/request-context(|/.*)$": "<rootDir>/libs/request-context/src",
      "^@libs/soft-delete(|/.*)$": "<rootDir>/libs/soft-delete/src",
      "^@libs/twilio(|/.*)$": "<rootDir>/libs/twilio/src",
      "^@libs/orm-core(|/.*)$": "<rootDir>/libs/orm-core/src",
      "^@libs/orm(|/.*)$": "<rootDir>/libs/orm/src",
      "^@libs/security(|/.*)$": "<rootDir>/libs/security/src",
      "^@libs/test(|/.*)$": "<rootDir>/libs/test/src",
      "^@libs/microservice-event(|/.*)$": "<rootDir>/libs/microservice-event/src/$1",
      "^@libs/healthcheck(|/.*)$": "<rootDir>/libs/healthcheck/src/$1",
      "^@libs/webauthn(|/.*)$": "<rootDir>/libs/webauthn/src/$1"
    }
  },
  "engines": {
    "node": "^22.0.0"
  }
